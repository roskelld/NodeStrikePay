    Serial.print(".");
  }
  Serial.println();
  Serial.print("Connected, IP address: ");
  Serial.println(WiFi.localIP());
  
  //initialise GPIO D1
  // Used for sending signal when payment detected
  pinMode(D1, OUTPUT);
  
  // Clear Screen and Draw Default Image
  display.drawBitmap(lightningbolt, 0, 0, 200, 200, GxEPD_WHITE); 
  display.update();
  
  //request initial invoice from ACINQ STRIKE  
  requestInvoice();
}


//Main loop
void loop() {
  //Checks to see if the QR has been paid
  if ( PAID == true ){
    Serial.println("SENDING PAID SIGNAL");
    // Send signal 
    digitalWrite(D1, HIGH);
    delay(100);
    digitalWrite(D1, LOW);
    delay(500);
    
    //request invoice from ACINQ STRIKE  
    requestInvoice(); 
  } else {
    Serial.println("Waiting for QR to be paid");
    // Delay between calls to server 
    delay(500);
    checkpay();
  }
}
// END LOOP

// Ping ACINQ STRIKE to see if payment has happened
void checkpay () {
  Serial.println("SENDING PAYMENT CHECK REQUEST");
  client.beginRequest();
  client.get("/api/v1/charges?size=1");
  client.sendBasicAuth(strikeapikey, strikeapikey);   
  client.endRequest();
  JsonObject& root = jsonBuffer.parseObject(client.responseBody().substring(1, client.responseBody().length() -1));
  if ( root["paid"] == "true" ) {
    PAID = true;
  } else {
    PAID = false;
  }
  jsonBuffer.clear();
}

// Request new invoice from ACINQ STRIKE
void requestInvoice() {
  client.beginRequest();
  client.post("/api/v1/charges");
  client.sendHeader("Content-Type", "application/x-www-form-urlencoded");
  client.sendHeader("Content-Length", postData.length());
  client.sendBasicAuth(strikeapikey, strikeapikey); 
  client.beginBody();
  client.print(postData);
  client.endRequest();
  JsonObject& root = jsonBuffer.parseObject(client.responseBody());
  if ( root["paid"] == "true" ) {
    PAID = true;
  } else {
    PAID = false;
  }
  String code = root["payment_request"];
  code.toUpperCase();
  Serial.println("HERE COMES THE BOLT11");
  Serial.println(code);  
  // Generate QRCode from Bolt11
  qrmaker( code );
  jsonBuffer.clear();
}

// QR maker function
void qrmaker(String code){

  char codeArray[code.length() + 1];
  code.toCharArray(codeArray, code.length() + 1);

  QRCode qrcode;
  uint8_t qrcodeData[qrcode_getBufferSize(11)];
  qrcode_initText(&qrcode, qrcodeData, 11, 0, codeArray );

  display.fillScreen(GxEPD_WHITE);
  // Height
  for (uint8 y = 0; y < qrcode.size; y++) {
    for (uint8 sy = 0; sy < scale; sy++) {
      // Width
      for (uint8 x = 0; x < qrcode.size; x++) {
        for (uint8 sx = 0; sx < scale; sx++) {
          if (qrcode_getModule(&qrcode, x, y)) {
            display.drawPixel( 
              border + (x * scale) + sx,
              border + (y * scale) + sy, 
              0x00 );          
          } else {          
            display.drawPixel( 
              border + (x * scale) + sx, 
              border + (y * scale) + sy, 
              0xff );          
          }
        }        
      }
    }
  }
  display.update(); 
}

